    <script type="text/javascript">
        var x = '@Model.Lfrom';
        var y = '@(Model.Lto)';
        var px = @(Model.Location_x);
        var py = @(Model.Location_y);
        console.log(x);
        console.log(y);
        //121.48789949, 31.24916171
        //121.413949,31.176527
        //121.360938,31.235822
        var map = new BMap.Map("l-map");
        map.centerAndZoom(new BMap.Point(121.360938, 31.235822), 12);
        var point = new BMap.Point(px, py);
        map.centerAndZoom(point, 15);
        var marker = new BMap.Marker(point);   
        map.addOverlay(marker);              
        marker.setAnimation(BMAP_ANIMATION_BOUNCE);  
        var point2 = new BMap.Point(121.248404, 31.254732);
        var marker2 = new BMap.Marker(point2);
        map.addOverlay(marker2);    
        var point3 = new BMap.Point(121.309123, 31.256787);
        var marker3 = new BMap.Marker(point3);
        map.addOverlay(marker3);    
        var transit = new BMap.DrivingRoute(map, {
            renderOptions: {
                map: map,
                panel: "r-result",
                enableDragging: true //起终点可进行拖拽
            },
        });
        transit.search(x, y);
        map.enableScrollWheelZoom(true);
    </script>

